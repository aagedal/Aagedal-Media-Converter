          }
        }
      }
    },
    "PRESET_VIDEO_LOOP_DESCRIPTION" : {
      "comment" : "Description for VideoLoop preset",
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Optimized for seamless silent loops. Encodes with x264 at CRF 23 (3–9 Mbps variable bitrate), strips audio, and limits the shortest edge to 1080 px for web playback."
          }
        },
        "nb" : {
          "stringUnit" : {
