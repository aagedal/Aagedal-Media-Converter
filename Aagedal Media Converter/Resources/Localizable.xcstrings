    "PRESET_ANIMATED_AVIF_DESCRIPTION" : {
      "comment" : "Description for Animated AVIF preset",
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "File extension: .avif | A modern, highly efficient image format with animation support. Uses AV1 codec for compression, specifically the svtav1 encoder. Automatically deinterlaces if needed using yadif=3. Limits resolution to 720 pixels on short end."
          }
        },
        "nb" : {
          "stringUnit" : {
