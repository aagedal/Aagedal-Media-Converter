    "PRESET_HEVC_PROXY_DESCRIPTION" : {
      "comment" : "Description for HECV Proxy 1080p preset",
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Proxy format based on HEVC codec. 10-bit 6 Mbps. Retains all audio channels as uncompressed PCM."
